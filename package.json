{
  "name": "multichain-manager",
  "description": "A simple management client for Multichain Blockchains",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "Roscoe van der Boom",
    "email": "roscoevanderboom@protonmail.com",
    "url": "https://github.com/roscoevanderboom"
  },
  "scripts": {
    "start": "rescripts start",
    "electron": "electron .",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder"
  },
  "build": {
    "productName": "Multichain Manager",
    "copyright": "Copyright Â© 2019 Roscoe van der Boom",
    "icon": "public/icon512.png",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "classnames": "^2.2.6",
    "electron-dl": "^3.0.2",
    "electron-is-dev": "^1.2.0",
    "extract-zip": "^2.0.1",
    "multichain-node": "^2.2.0-version1.0.4",
    "notistack": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "tar": "^6.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "^5.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "typescript": "^4.0.5",
    "wait-on": "^5.2.0"
  }
}
